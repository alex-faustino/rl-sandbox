<robot name="monoped">
    <link name="base">
	    <visual>
	      <geometry>
	        <box size="0.20 0.20 0.30"/>
	      </geometry>
	      <material name="blue">
	        <color rgba="0.3 0.3 0.3 1.0"/>
	      </material>
	    </visual>
        <collision>
          <geometry>
            <box size="0.20 0.20 0.30"/>
          </geometry>
        </collision>
        <inertial>
          <mass value="1"/>
          <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
    </link>
    
    <link name="hipassembly">
	    <visual>
	      <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
	      <geometry>
	        <mesh filename="package://meshes/leg/hipassembly.dae" scale="1 1 1"/>
	      </geometry>
	      <material name="white"/>
	    </visual>
        <collision>
          <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
          <geometry>
            <mesh filename="package://meshes/leg/hipassembly.dae" scale="1 1 1"/>
          </geometry>
        </collision>
        <inertial>
          <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
          <mass value="0.0"/>
          <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
    </link>
    <link name="upperleg">
	    <visual>
	      <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
	      <geometry>
	        <mesh filename="package://meshes/leg/upperleg.dae" scale="1 1 1"/>
	      </geometry>
	      <material name="blue"/>
	    </visual>
        <collision>
          <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
          <geometry>
            <mesh filename="package://meshes/leg/upperleg.dae" scale="1 1 1"/>
          </geometry>
        </collision>
        <inertial>
          <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
          <mass value="0.0"/>
          <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
    </link>
    <link name="lowerleg">
	    <visual>
	      <geometry>
	        <mesh filename="package://meshes/leg/lowerleg.dae" scale="1 1 1"/>
	      </geometry>
	      <material name="blue"/>
	    </visual>        
        <collision>
          <geometry>
            <mesh filename="package://meshes/leg/lowerleg.dae" scale="1 1 1"/>
          </geometry>
        </collision>
        <inertial>
          <mass value="0.0"/>
          <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
    </link>
    
    <link name="hand">
	    <visual>
          <origin xyz="0 0.625 0" rpy="1.57 0 0"/> 
	      <geometry>
	        <cylinder radius="0.025" length="1.25"/>
	      </geometry>
	      <material name="invis_blue">
	        <color rgba="0.3 0.3 0.3 0.0"/>
	      </material>
	    </visual>        
        <collision>
          <origin xyz="0 0.625 0" rpy="1.57 0 0"/> 
          <geometry>
            <cylinder radius="0.025" length="1.25"/>
          </geometry>
        </collision>
        <inertial>
          <mass value="0.01"/>
          <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
    </link>
    
    <link name="slideBar">
    <visual>
      <geometry>
        <box size="0.05 0.05 30"/>
      </geometry>
      <origin xyz="0 1.25 0" rpy="0 0 0"/>
      <material name="green">
        <color rgba="0 0.8 .8 0"/>
      </material>
    </visual>
    <inertial>
      <mass value="0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
   </link>
  
    <joint name="slider_to_hand" type="prismatic">
        <axis xyz="0 0 1"/>
        <origin xyz="0.0 0.0 0.0"/>
        <parent link="slideBar"/>
        <child link="hand"/>
        <limit effort="1000.0" lower="-15" upper="15" velocity="5"/>
    </joint>
    
    <joint name="hand_joint" type="revolute">
        <origin xyz="0.0 0.0 0" rpy="0 0 0"/>
        <parent link="hand"/>
        <child  link="base"/>
        <limit effort="200" lower="-1.6" upper="1.6" velocity="1.0"/>
        <axis xyz="0 1 0"/>
    </joint>
    
    <joint name="haa_joint" type="fixed">
        <origin xyz="0.0 0.0 -0.15000" rpy="2.0344439357957036 1.5707962290814481 -1.1071487177940917"/>
        <parent link="base"/>
        <child  link="hipassembly"/>
        <limit effort="200" lower="-1.6" upper="1.6" velocity="1.0"/>
        <axis xyz="0 0 1"/>
    </joint>
    <joint name="hfe_joint" type="revolute">
        <origin xyz="0.08000 0.00000 0.00000" rpy="1.5707963705062866 -0.0 0.0"/>
        <parent link="hipassembly"/>
        <child  link="upperleg"/>
        <limit effort="200" lower="-1.6" upper="1.6" velocity="1.0"/>
        <axis xyz="0 0 1"/>
    </joint>
    <joint name="kfe_joint" type="revolute">
        <origin xyz="0.35000 0.00000 0.00000" rpy="0.0 0.0 0.0"/>
        <parent link="upperleg"/>
        <child  link="lowerleg"/>
        <limit effort="200" lower="-1.6" upper="1.6" velocity="1.0"/>
        <axis xyz="0 0 1"/>
    </joint>
</robot>

