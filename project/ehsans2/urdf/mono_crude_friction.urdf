<robot name="monoped">
        
     <link name="slideBar">
         <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.005 0.005 30"/>
            </geometry>
            <material name="green">
            <color rgba="0 0.8 .8 0"/>
            </material>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
    </link>
    
    <link name="base">
        <contact>
            <friction_anchor/>
            <stiffness value="30000.0"/>
            <damping value="1000.0"/>
            <spinning_friction value=".3"/>
            <lateral_friction value="1"/>
        </contact>
	    <visual>
          <origin xyz="0 0 0" rpy="0 0 0"/>
	      <geometry>
	        <box size="0.30 0.30 0.30"/>
	      </geometry>
	      <material name="blue">
	        <color rgba="0.3 0.3 0.3 1.0"/>
	      </material>
	    </visual>
        <collision>
          <geometry>
            <box size="0.20 0.20 0.30"/>
          </geometry>
          <origin xyz="0 0 0" rpy="0 0 0"/>
        </collision>
        <inertial>
          <mass value="1"/>
          <origin xyz="0 0 0" rpy="0 0 0"/>
          <inertia ixx="0.015" ixy="0.0" ixz="0.0" iyy="0.015" iyz="0.0" izz="0.015"/>
        </inertial>
    </link>
    
    <link name="upperleg">
        <contact>
            <friction_anchor/>
            <stiffness value="30000.0"/>
            <damping value="1000.0"/>
            <spinning_friction value=".3"/>
            <lateral_friction value="1"/>
        </contact>
	    <visual>
	      <geometry>
	        <box size="0.02 0.02 0.3"/>
	      </geometry>
          <origin xyz="0.0 0.0 -0.16" rpy="0 0 0"/>
	      <material name="blue"/>
	    </visual>
        <collision>
          <geometry>
            <box size="0.02 0.02 0.3"/>
          </geometry>
          <origin xyz="0.0 0.0 -0.16" rpy="0 0 0"/>
        </collision>
        <inertial>
          <mass value="0.1"/>
          <origin xyz="0.0 0.0 -0.16" rpy="0 0 0"/>
          <inertia ixx="0.003"  ixy="0"  ixz="0" iyy="0.003" iyz="0" izz="0.00000666666" />
        </inertial>
    </link>
    
    <link name="lowerleg">
        <contact>
            <friction_anchor/>
            <stiffness value="30000.0"/>
            <damping value="1000.0"/>
            <spinning_friction value=".3"/>
            <lateral_friction value="1"/>
        </contact>
	    <visual>
	      <geometry>
	        <box size="0.02 0.02 0.3"/>
	      </geometry>
          <origin xyz="0.0 0.0 -0.16" rpy="0 0 0"/>
	      <material name="blue"/>
	    </visual>        
        <collision>
          <geometry>
            <box size="0.02 0.02 0.3"/>
          </geometry>
          <origin xyz="0.0 0.0 -0.16" rpy="0 0 0"/>
        </collision>
        <inertial>
          <mass value="0.1"/>
          <origin xyz="0.0 0.0 -0.16" rpy="0 0 0"/>
          <inertia ixx="0.003"  ixy="0"  ixz="0" iyy="0.003" iyz="0" izz="0.00000666666" />
        </inertial>
    </link>
  
    <joint name="slider_to_base" type="prismatic">
        <axis xyz="0 0 1"/>
        <origin xyz="0.0 0.0 0"/>
        <parent link="slideBar"/>
        <child link="base"/>
        <dynamics damping="0.0" friction="0.01"/>
        <limit effort="200" lower="-15" upper="15" velocity="5"/>
    </joint>
    
    <joint name="base_to_upperleg" type="revolute">
        <origin xyz="0.0 0.0 -0.16"/>
        <parent link="base"/>
        <child  link="upperleg"/>
        <limit effort="200" lower="-1.6" upper="1.6" velocity="1.0"/>
        <dynamics damping="0.0" friction="0.01"/>
        <axis xyz="0 1 0"/>
    </joint>
    <joint name="upperleg_to_lowerleg" type="revolute">
        <origin xyz="0.0 0.0 -0.32"/>
        <parent link="upperleg"/>
        <child  link="lowerleg"/>
        <limit effort="200" lower="-3.1" upper="3.1" velocity="1.0"/>
        <dynamics damping="0.0" friction="0.01"/>
        <axis xyz="0 1 0"/>
    </joint>
</robot>

